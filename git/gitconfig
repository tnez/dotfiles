[user]
  name = Travis Nesland
  email = tnesland@gmail.com
[github]
  user = tnez
[alias]
  a = add
  aa = add .
  b = checkout -b
  bd = branch -d
  c = commit
  ca = commit --amend
  cm = commit -m
  co = checkout
  d = diff
  f = fetch --prune
  ff = pull
  fp = push --force-with-lease
  h = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  m = merge
  p = !git_publish.sh
  r = rebase
  ri = rebase --interactive
  s = status
  z = stash
  zl = stash list
  zp = stash pop
  zz = stash save --include-untracked
[core]
  editor = vim
  excludesfile = ~/.gitignore
[diff]
  tool = vimdiff
[difftool]
  prompt = 0
[merge]
  tool = vimdiff
  conflictstyle = diff3
[init]
  templatedir = ~/.git_template
[ghi]
	token = !security find-internet-password -a tnez -s github.com -l 'ghi token' -w
