[github]
  user = tnez
[alias]
  a = add
  aa = add .
  b = checkout -b
  bd = branch -d
  c = commit
  ca = commit --amend
  cm = commit -m
  co = checkout
  copr = pr checkout
  d = diff
  l = log --oneline --graph
  m = merge
  p = !git_publish.sh
  pf = push --force-with-lease
  r = rebase
  ri = rebase --interactive
  s = status
  z = stash
  zl = stash list
  zp = stash pop
  zz = stash save --include-untracked
[core]
  editor = vim
  excludesfile = ~/.gitignore
[diff]
  tool = vimdiff
[difftool]
  prompt = 0
[fetch]
  prune = true
[merge]
  tool = vimdiff
  conflictstyle = diff3
[rebase]
  autosquash = true
[init]
  templatedir = ~/.git_template
[ghi]
  token = !security find-internet-password -a tnez -s github.com -l 'ghi token' -w
