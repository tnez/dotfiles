<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.tnez.tmux-home</string>
  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/tmux</string>
    <string>new-session</string>
    <string>-d</string>
    <string>-s</string>
    <string>home</string>
    <string>-c</string>
    <string>/Users/tnez</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/tmux-home.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/tmux-home.err</string>
</dict>
</plist>
