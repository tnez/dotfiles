{
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.codeActionsOnSave": {
		"source.organizeImports": false
	},
	"editor.cursorSurroundingLines": 8,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[python]": {
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.formatOnSave": true,
		"editor.wordBasedSuggestions": false,
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.minimap.enabled": false,
	"editor.roundedSelection": false,
	"editor.scrollBeyondLastLine": false,
	"editor.suggestSelection": "first",
	"editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
	"files.exclude": {
		"**/.venv": true,
		"**/node_modules": true
	},
	"git.autofetch": true,
	"git.confirmSync": true,
	"git.useEditorAsCommitInput": true,
	"git.mergeEditor": true,
	"leetcode.defaultLanguage": "python3",
	"leetcode.workspaceFolder": "/Users/tnez/Code/tnez/leet",
	"leetcode.filePath": {
		"default": {
			"folder": "",
			"filename": "${id}.${kebab-case-name}.${ext}"
		}
	},
	"projectManager.git.baseFolders": ["$home/Code", "$home/workspace"],
	"projectManager.git.maxDepthRecursion": 3,
	"projectManager.sortList": "Recent",
	"python.testing.pytestEnabled": true,
	"terminal.integrated.commandsToSkipShell": [
		"workbench.action.navigateRight",
		"workbench.action.navigateLeft"
	],
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		},
		{
			"before": ["k", "k"],
			"after": ["<Esc>"]
		},
		{
			"before": ["k", "j"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindings": [
		{
			"before": ["<C-d>"],
			"after": ["<C-d>", "z", "z"]
		},
		{
			"before": ["<C-u>"],
			"after": ["<C-u>", "z", "z"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.showDefinitionPreviewHover"],
			"silent": true
		},
		{
			"before": ["H"],
			"commands": ["workbench.action.previousEditorInGroup"]
		},
		{
			"before": ["L"],
			"commands": ["workbench.action.nextEditorInGroup"],
			"silent": true
		},
		{
			"before": ["g", "]"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["g", "["],
			"commands": ["editor.action.marker.prev"]
		},
		{
			"before": ["<leader>", "b", "c"],
			"commands": [":q"]
		},
		{
			"before": ["<leader>", "b", "d"],
			"commands": [":q"]
		},
		{
			"before": ["<leader>", "b", "q"],
			"commands": [":q"]
		},
		{
			"before": ["<leader>", "b", "s"],
			"commands": [":w"]
		},
		{
			"before": ["<leader>", "g", "b"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "g", "f"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "g", "g"],
			"commands": ["workbench.view.scm"]
		},
		{
			"before": ["<leader>", "g", "p"],
			"commands": ["projectManager.listProjects"]
		},
		{
			"before": ["<leader>", "g", "t"],
			"commands": ["workbench.action.createTerminalEditor"]
		},
		{
			"before": ["<leader>", "<leader>"],
			"commands": ["workbench.action.showCommand"]
		}
	],
	"vim.foldfix": true,
	"vim.leader": "<space>",
	"vim.smartRelativeLine": true,
	"vim.useCtrlKeys": true,
	"vim.handleKeys": {
		"<C-b>": false
	},
	"vim.useSystemClipboard": true,
	"window.commandCenter": true,
	"workbench.editor.enablePreview": false,
	"workbench.list.defaultFindMode": "filter",
	"workbench.colorTheme": "Everforest Dark",
	"workench.iconTheme": "material-icon-theme",
	"workbench.preferredDarkColorTheme": "Everforest Dark",
	"workbench.preferredLightColorTheme": "Everforest Light",
	"workbench.settings.editor": "json",
	"terminal.integrated.letterSpacing": 1,
	"githubPullRequests.pullBranch": "never",
	"explorer.confirmDragAndDrop": false,
	"eslint.probe": [
		"html",
		"javascript",
		"javascriptreact",
		"markdown",
		"mdx",
		"typescript",
		"typescriptreact",
		"vue"
	],
	"eslint.validate": [
		"html",
		"javascript",
		"javascriptreact",
		"markdown",
		"mdx",
		"typescript",
		"typescriptreact",
		"vue"
	],
	"eslint.options": {
		"extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
	},
	"githubPullRequests.pushBranch": "always",
	"workbench.statusBar.visible": false,
	"breadcrumbs.enabled": false,
	"editor.stickyScroll.enabled": true,
	"workbench.activityBar.visible": false,
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "readme"
}
